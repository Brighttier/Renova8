# Renova8 Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# Gemini API Configuration
# ===========================================
VITE_API_KEY=your_gemini_api_key_here

# ===========================================
# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General
# ===========================================
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# ===========================================
# Optional: Development Settings
# ===========================================
# Set to "true" to use Firebase emulators in development
VITE_USE_EMULATORS=false

# ===========================================
# Sentry Error Monitoring (Frontend)
# Get your DSN from https://sentry.io/settings/projects/renovatemysite-frontend/keys/
# ===========================================
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# ===========================================
# Sentry Source Maps Upload (Build/CI only)
# Get auth token from https://sentry.io/settings/auth-tokens/
# ===========================================
SENTRY_AUTH_TOKEN=
SENTRY_ORG=renovatemysite
SENTRY_PROJECT=renovatemysite-frontend
